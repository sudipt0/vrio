{
  "name": "sudipta/vrio",
  "version": "1.0.2",
  "description": "Laravel package for Vrio CRM",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Sudipta Mondal",
      "email": "sudip1992@gmail.com"
    }
  ],
  "require": {
    "php": "^8.1",
    "guzzlehttp/guzzle": "^7.7",
    "illuminate/support": "^10.0 || ^11.0 || ^12.0"
  },
  "autoload": {
    "psr-4": {
      "Sudipta\\Vrio\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Sudipta\\Vrio\\VrioServiceProvider"
      ],
      "aliases": {
        "Vrio": "Sudipta\\Vrio\\Facades\\Vrio"
      }
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
